generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model Trip {
    id            String   @id @default(cuid())
    destination   String
    starts_at     DateTime
    ends_at       DateTime
    ids_confirmed Boolean  @default(false)
    created_at    DateTime @default(now())

    @@map("trips")
}
